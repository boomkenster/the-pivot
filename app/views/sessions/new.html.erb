<div class="container">
  <div class="stitched">
    <%= form_for(:session, url:login_path) do |f| %>
      <%= f.label "Email:" %>
      <%= f.text_field :email %>

      <%= f.label "Password:" %>
      <%= f.password_field :password %>

      <%= f.submit 'Login' %>
    <% end %>
  </div>
  
</div>
