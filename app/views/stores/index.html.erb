<div class="row">
  <div class="col-md-12"><h1 id="stores-header">Shops</h1></div>
</div>

<% @stores.each do |store| %>
  <div class="col-md-3 stores">
  <h5><%= link_to store.name, store_items_path(store: store.slug) %></h5>
  <%= link_to image_tag(store.items.first.attachment.url), store_items_path(store: store.slug) %>



    </div>
<% end %>

